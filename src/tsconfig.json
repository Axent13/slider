{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    // "noUncheckedIndexedAccess": true, - выдает ошибку, мол, нет такого свойства...
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}